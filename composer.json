  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "apcu-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "ReportingApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReportingApi\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "encore:production": "encore production"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "quality": [
      "php-cs-fixer fix",
      "phpstan analyse",
      "rector process --dry-run"
    ],
    "test": [
      "phpunit",
      "codecept run",
      "behat"
    ],
    "security": [
      "composer audit"
    ],
    "reporting": [
      "bin/console reporting:generate-scheduled",
      "bin/console reporting:refresh-cubes",
      "bin/console reporting:cleanup-cache",
      "bin/console reporting:validate-data",
      "bin/console reporting:optimize-queries"
    ],
    "dashboard": [
      "bin/console dashboard:compile-executive",
      "bin/console dashboard:refresh-kpis",
      "bin/console dashboard:generate-insights",
      "bin/console dashboard:update-realtime"
    ],
    "bi": [
      "bin/console bi:aggregate-data",
      "bin/console bi:build-cubes",
      "bin/console bi:refresh-materialized-views",
      "bin/console bi:validate-metrics"
    ],
    "export": [
      "bin/console export:generate-monthly",
      "bin/console export:compliance-reports",
      "bin/console export:executive-summary"
    ],
    "performance": [
      "bin/console cache:warmup",
      "bin/console reporting:precompute-aggregations",
      "bin/console dashboard:optimize-assets",
      "bin/console bi:rebuild-indexes"
    ],
    "monitoring": [
      "bin/console reporting:health-check",
      "bin/console reporting:metrics-collect",
      "bin/console reporting:sla-report"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.0.*",
      "docker": true
    }
  }
}
